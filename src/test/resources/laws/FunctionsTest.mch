MACHINE FunctionsTest
VARIABLES x
INVARIANT x : 1..3
INITIALISATION x := 1
OPERATIONS
foo = PRE 1 = 1 THEN x := 2 END 
ASSERTIONS
 dom(%x.(x:{1,2,3}| 1)) = {1,2,3};
 ran(%x.(x:{1,2,3}| x)) = {1,2,3};
 
 %x.(x:{1,2,3}|1) : {1,2,3} --> {1};
 %x.(x:{1,2}|x) : {1,2} >-> {1,2,3};
 %x.(x:{1,2,3}|1) : {1,2,3} -->> {1};
 %x.(x:{1,2}|x) : {1,2} >->> {1,2};

 %x.(x:{1,2}|1) : {1,2,3} +-> {1};
 %x.(x:{1}|1) : {1,2,3} >+> {1};
 %x.(x:{1,2}|1) : {1,2,3} +->> {1};
 %x.(x:{1,2}|x) : {1,2,3} >+>> {1,2}
END